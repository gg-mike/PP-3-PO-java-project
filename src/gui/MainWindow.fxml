                <Tab fx:id="mapTab" text="Map">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="531.0" prefWidth="635.0">
                        <Pane fx:id="map" onMouseDragged="#dragMap" onMousePressed="#startDragMap" onScroll="#zoomMap" prefHeight="500.0" prefWidth="600.0">
                     <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#285cc4" height="500.0" stroke="BLACK" strokeLineCap="BUTT" strokeLineJoin="ROUND" strokeType="INSIDE" width="600.0" />
                            <Polygon fill="#327814" points="125.0,  75.0, 175.0,  75.0, 225.0, 125.0, 375.0, 125.0, 375.0, 125.0, 375.0,  75.0, 425.0,  75.0, 475.0, 125.0, 525.0, 125.0, 525.0, 175.0, 425.0, 175.0, 425.0, 225.0, 325.0, 225.0, 275.0, 175.0, 175.0, 175.0, 125.0, 125.0" />
                            <Polygon fill="#327814" points="125.0, 425.0, 325.0, 425.0, 325.0, 325.0, 275.0, 275.0, 225.0, 275.0, 175.0, 225.0, 125.0, 225.0,  75.0, 275.0,  75.0, 375.0" />
                            <Polygon fill="#327814" points="425.0, 475.0, 525.0, 475.0, 525.0, 375.0, 475.0, 375.0, 425.0, 425.0" />
                        </Pane>
                  <Button fx:id="resetMapButton" layoutX="570.0" layoutY="470.0" mnemonicParsing="false" onAction="#resetMap" opacity="0.5" prefHeight="30.0" prefWidth="30.0" text="🔄" />
                    </AnchorPane>
                </Tab>
                <Tab fx:id="infoTab" text="Info">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <ScrollPane fx:id="infoTabScrollPane" hbarPolicy="NEVER" prefHeight="534.0" prefWidth="475.0">
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                        </ScrollPane>
                    </AnchorPane>
                </Tab>
            </TabPane>
            <VBox fx:id="objectDataVBox" disable="true" prefHeight="529.0" prefWidth="400.0" HBox.hgrow="NEVER">
                <Label fx:id="nameLabel" minHeight="40.0" prefHeight="40.0" prefWidth="331.0" text="Name:">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                    <padding>
                        <Insets left="10.0" />
                    </padding>
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <ToolBar minHeight="40.0" prefHeight="40.0" prefWidth="200.0">
                    <Button fx:id="objectButton1" mnemonicParsing="false" text="Button1" visible="false" />
               <Button fx:id="objectButton2" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" text="Button2" visible="false" />
                </ToolBar>
            <TableView fx:id="objectDataTable" prefHeight="449.0" prefWidth="292.0" VBox.vgrow="NEVER">
              <columns>
                <TableColumn fx:id="paramColumn" prefWidth="117.0" resizable="false" sortable="false" text="Parameter">
                    <cellValueFactory>
                        <PropertyValueFactory property="param" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="valueColumn" prefWidth="282.0" resizable="false" sortable="false" text="Value">
                    <cellValueFactory>
                        <PropertyValueFactory property="value" />
                    </cellValueFactory>
                </TableColumn>
              </columns>
            </TableView>
            </VBox>
        </HBox>
    </VBox>
</AnchorPane>
